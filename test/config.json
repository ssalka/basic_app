{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts",
    "tsx"
  ],
  "moduleNameMapper": {
    "\\.less$": "<rootDir>/node_modules/jest-css-modules"
  },
  "moduleDirectories": [
    "node_modules",
    "."
  ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/test/index/"
  ],
  "testRegex": "(/test/.*\\-spec)\\.(j|t)sx?$",
  "transform": {
    "\\.gql$": "<rootDir>/node_modules/jest-transform-graphql",
    ".js": "<rootDir>/node_modules/webpack-babel-jest",
    ".tsx?": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "setupTestFrameworkScriptFile": "<rootDir>/index.js",
  "verbose": true
}
