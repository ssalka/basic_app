  "engines": {
    "node": "^8.4.0",
    "npm": "^5.3.0"
  },
  "scripts": {
    "dev": "NODE_PATH=. ts-node -P config/tsconfig.server.json index",
    "format": "prettier --config config/prettier.json --write",
    "format:all": "npm run format -- 'lib/**/*.ts{,x}' 'src/**/*.ts{,x}' 'test/**/*.ts{,x}'",
    "lint": "tslint -c config/tslint.json -t stylish",
    "lint:all": "npm run lint -- 'lib/**/*.ts{,x}' 'src/**/*.ts{,x}' 'test/**/*.ts{,x}'",
    "precommit": "lint-staged",
    "start": "NODE_PATH=. ts-node -F -P config/tsconfig.server.json index",
    "test": "NODE_ENV=test NODE_PATH=. jest --config config/jest.json --noStackTrace"
  },
  "lint-staged": {
    "*.ts{,x}": [
      "format",
      "lint",
      "git add"
    ]
  },
